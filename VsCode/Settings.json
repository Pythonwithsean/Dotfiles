{
  "github.copilot.nextEditSuggestions.enabled": true,
  "editor.fontFamily": "IBM Plex Mono Medm",
  "editor.cursorStyle": "block",
  "editor.fontWeight": "800",
  "vim.handleKeys": {
    "<C-b>": false,
    "<C-j>": false
  },
  "telemetry.feedback.enabled": false,
  "telemetry.editStats.enabled": false,
  "editor.formatOnSave": true,
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "git.enableSmartCommit": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "editor.minimap.renderCharacters": false,
  "breadcrumbs.enabled": true,
  "editor.minimap.size": "fit",
  "editor.wordWrap": "on",
  "workbench.sideBar.location": "right",
  "chat.agent.maxRequests": 50,
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.fontSize": 19,
  "editor.minimap.sectionHeaderFontSize": 1,
  "debug.console.fontSize": 18,
  "window.zoomLevel": 1,
  "editor.lineHeight": 1.6,
  "editor.letterSpacing": 0.9,
  "errorLens.fontWeight": "800",
  "terminal.integrated.fontLigatures.enabled": true,
  "editor.fontLigatures": true,
  "chat.fontSize": 18,
  "chat.editor.fontSize": 18,
  "terminal.integrated.fontSize": 18,
  "chat.editor.fontWeight": "800",
  "terminal.integrated.fontWeight": "800",
  "workbench.secondarySideBar.defaultVisibility": "visible",
  "workbench.colorCustomizations": {
    "editor.background": "#262626",
    "sideBar.background": "#262626",
    "sideBar.foreground": "#e6e6e6",
    "sideBarSectionHeader.background": "#262626",
    "list.dropBackground": "default",
    "terminal.background": "#262626",
    "panel.background": "#262626",
    "activityBar.background": "#262626",
    "menu.background": "#282828",
    "menu.foreground": "#ffffff",
    "titleBar.activeBackground": "#262626",
    "titleBar.activeForeground": "#ffffff",
    "titleBar.inactiveBackground": "#262626",
    "titleBar.inactiveForeground": "#cccccc",
    "statusBar.background": "#262626",
    "breadcrumb.background": "#262626",
    "breadcrumb.foreground": "#e6e6e6",
    "breadcrumb.focusForeground": "#ffffff",
    "breadcrumb.activeSelectionForeground": "#ffffff",
    "editorGroupHeader.tabsBackground": "#262626",
    "tab.activeBackground": "#262626",
    "tab.inactiveBackground": "#262626",
    "tab.activeForeground": "#ffffff",
    "tab.inactiveForeground": "#cfcfcf",
    "tab.hoverBackground": "#2b2b2b",
    "tab.border": "#262626",
    "[Minimal]": {
      "menu.foreground": "#fff",
      "panel.background": "#222222",
      "icon.foreground": "#fff"
    }
  },
  "terminal.integrated.gpuAcceleration": "off",
  "redhat.telemetry.enabled": true,

  "telemetry.telemetryLevel": "off",
  "workbench.activityBar.location": "top",
  "github.copilot.enable": {
    "*": false,
    "plaintext": false,
    "markdown": false,
    "scminput": false
  },
  "explorer.confirmPasteNative": false,
  "workbench.iconTheme": "charmed-icons",
  "window.customTitleBarVisibility": "windowed",
  "workbench.colorTheme": "FireBox"
}
